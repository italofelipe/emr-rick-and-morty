{
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
  "rules": {
    "semi": ["error", "always"],
    "sort-imports": [
      "error",
      {
        "ignoreCase": false,
        "ignoreDeclarationSort": false,
        "ignoreMemberSort": false,
        "memberSyntaxSortOrder": ["multiple", "all", "none", "single"],
        "allowSeparatedGroups": false
      }
    ],

    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
